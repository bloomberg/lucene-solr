        "value" : "${myOcc}",
        "required" : false
    }
}, {
    "name":"originalScore",
    "type":"org.apache.solr.ltr.feature.impl.OriginalScoreFeature",
    "store": "fstore3",
    "params":{}
}, {
    "name" : "popularity",
    "type" : "org.apache.solr.ltr.feature.impl.ValueFeature",
