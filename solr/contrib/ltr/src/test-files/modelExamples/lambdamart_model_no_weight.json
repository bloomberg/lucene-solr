{
    "class":"org.apache.solr.ltr.model.LambdaMARTModel",
    "name":"lambdamartmodel_no_weight",
    "features":[
        { "name": "matchedTitle"},
        { "name": "constantScoreToForceLambdaMARTScoreAllDocs"}
    ],
    "params":{
        "trees": [
            {
                "tree": {
                    "feature": "matchedTitle",
                    "threshold": 0.5,
                    "left" : {
                        "value" : -100
                    },
                    "right": {
                        "value" : 75
                    }
                }
            }
        ]
    }
}
