{
	"class":"org.apache.solr.ltr.ranking.RankSVMModel",
	"name":"fqmodel",
	"features":[
        {
            "name":"matchedTitle",
            "norm": {
                "class":"org.apache.solr.ltr.feature.norm.impl.MinMaxNormalizer",
                "params":{ "min":"0.0f", "max":"10.0f" }
            }
        },
        { "name":"popularity"}
	],
	"params":{
	  "weights": {
	    "matchedTitle": 0.5,
	    "popularity": 0.5
	  }
	}
}
